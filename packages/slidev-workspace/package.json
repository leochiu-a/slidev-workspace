    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.11",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.1.3",
    "vue-tsc": "^3.0.3"
