  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leochiu-a/slidev-workspace"
  },
  "author": "Author Name <author.name@mail.com>",
  "bin": {
    "slidev-workspace": "./dist/cli.js"
  },
