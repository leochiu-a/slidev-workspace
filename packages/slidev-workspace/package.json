{
  "name": "slidev-workspace",
  "version": "0.5.1",
  "description": "A workspace tool for managing multiple Slidev presentations with API-based content management",
  "type": "module",
  "license": "MIT",
  "homepage": "https://leochiu-a.github.io/slidev-workspace",
  "bugs": {
